{
    "compilerOptions": {
      "target": "esnext",
      "module": "nodenext",
      "lib": ["esnext"],
      "types": [],
      "sourceMap": true,
      "declaration": true,
      "declarationMap": true,
      "strict": true,
      "jsx": "react-jsx",
      "verbatimModuleSyntax": true,
      "isolatedModules": true,
      "noUncheckedSideEffectImports": true,
      "moduleDetection": "force",
      "skipLibCheck": true,
      "noUncheckedIndexedAccess": true,
      "exactOptionalPropertyTypes": true
    }
  }